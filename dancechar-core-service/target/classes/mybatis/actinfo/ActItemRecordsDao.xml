<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.litian.dancechar.core.biz.activity.itemrecords.dao.inf.ActItemRecordsDao">

    <select id="findList" parameterType="com.litian.dancechar.core.biz.activity.itemrecords.dto.ActItemRecordReqDTO"
            resultType="com.litian.dancechar.core.biz.activity.itemrecords.dto.ActItemRecordsRespDTO">
        SELECT
        c.*
        FROM
        act_item_records c
        WHERE
        c.delete_flag = 0
    </select>
</mapper>
